{
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": ["transform-class-properties"],
      "only": [
        "./**/*.js",
        "node_modules/jest-runtime"
      ]
    },
    "development": {
      "plugins": [
        "transform-class-properties",
        "retainLines": true
      ],
      "presets": [
        "env",
        "es2015",
        "react",
        "stage-2"
      ]
    }
  }
}

