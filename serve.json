{
  "headers": [
    {
      "source" : "**/*.@(js|html)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
      }]
    }, {
      "source" : "404.html",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=300"
      }]
    }
  ]
}
