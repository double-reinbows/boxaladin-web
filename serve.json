{
  "headers": [
    {
      "source" : "**/*.@(js|html)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    },
    {
      "source" : "**/*.@(css)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=7200"
      }]
    }
  ]
}
